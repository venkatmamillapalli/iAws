{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Learing Cloudformation",
  "Resources": {
    "myvpcone": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.0.0.0/16",
        "Tags":[
          { "Key": "name", "Value":  "cloudformation" }
          ]
      }
    },
    "subweb": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myvpcone"
        },
        "CidrBlock": "10.0.0.0/24",
        
        "Tags":[
        { "Key": "name", "Value":  "public" }
        ]
      }
    },
    "websub": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "VpcId": {
          "Ref": "myvpcone"
        },
       "CidrBlock": "10.0.1.0/24",
       "Tags":[
        { "Key": "name", "Value":  "private" }
        ]
      }
    }
  }
}